import java.util.Scanner;

public class MainWhiletrue {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        int max = 0; // зберігає найбільший елемент послідовності

        while (true) {
            int x = scanner.nextInt();

            if (x == 0) {      // 0 — ознака кінця послідовності
                break;
            }

            if (x > max) {    // оновлюємо максимум
                max = x;
            }
        }

        System.out.println(max);
        scanner.close();
    }
}
